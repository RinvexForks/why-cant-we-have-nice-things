        "knplabs/github-api": "^1.6",
        "krakjoe/pthreads-polyfill": "dev-master",
        "league/commonmark": "^0.13.2",
        "league/container": "^2.2",
        "league/route": "^2.0",
        "league/tactician": "^1.0",
        "mnapoli/silly": "^1.3",
        "monolog/monolog": "^1.18",
